<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
  <Match>
    <Bug category="PERFORMANCE" />
  </Match>
  <Match>
    <Bug pattern="EI_EXPOSE_REP" />
  </Match>
  <Match>
    <Bug pattern="EI_EXPOSE_REP2" />
  </Match>
  <Match>
    <Bug pattern="DM_DEFAULT_ENCODING" />
  </Match>
  <Match>
    <Class name="frc.robot.Robot" />
    <And>
      <OR>
        <Bug pattern="ST_WRITE_TO_STATIC_FROM_INSTANCE_METHOD" />
        <Bug pattern="MS_EXPOSE_REP" />
      </OR>
    </And>
  </Match>
  <Match>
    <Class
      name="frc.spectrumLib.vision.LimelightHelpers$RawDetection" />
  </Match>
  <Match>
    <Class name="frc.spectrumLib.vision.LimelightHelpers" />
  </Match>
</FindBugsFilter>
